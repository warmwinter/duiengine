        <tab id="114" title="richedit">
          <duiedit id="1140" show="1" number="0" class="cls_edit" font="0004:隶书" inset="30,0,40,0" wordwrap="1"  crbg="FFFFFF"   pos="10,10,-10,|0" multilines="1" wantreturn="1" hscrollbar="0" vscrollbar="1" autovscroll="1" crtext="ff0000" align="left">richedit test 2012</duiedit>
          <check id="1142" pos="10,|10">check 1</check>
